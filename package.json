{
  "name": "vet-nurse-questions",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1"
  },
  "devDependencies": {
    "xo": "^0.17.1"
  },
  "xo": {
    "space": true,
    "ignores": [
      "app/assets/**",
      "govuk_modules/**",
      "public/**"
    ],
    "env": [
      "jquery",
      "browser"
    ],
    "rules": {
      "xo/no-process-exit": 0,
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "esnext": true
  }
}
