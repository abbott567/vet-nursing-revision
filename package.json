{
  "name": "vet-nurse-questions",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "testcoverage": "istanbul cover _mocha -- -u exports -R spec"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "bootstrap": "^3.3.7",
    "cookie-parser": "^1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jquery": "^3.1.1",
    "mongoose": "^4.8.1",
    "morgan": "~1.6.1",
    "nunjucks": "^3.0.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "require-dir": "^0.3.1",
    "run-sequence": "^1.2.2",
    "xo": "^0.17.1",
    "zombie": "^5.0.5"
  },
  "xo": {
    "space": true,
    "esnext": true,
    "env": [
      "jquery",
      "browser",
      "mocha"
    ],
    "rules": {
      "xo/no-process-exit": 0,
      "no-unused-vars": [
        2,
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    },
    "overrides": [
      {
        "files": [
          "**/frontend-js.js"
        ],
        "esnext": false
      }
    ]
  }
}
